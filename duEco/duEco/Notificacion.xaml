<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="duEco.Notificacion">
    <ContentPage.Content>
        <StackLayout>
            <!-- Place new controls here -->
            <Button x:Name="btnNotification" Text="Enviar Notificacion en 5 seconds" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" Clicked="BtnNotification_Clicked"/>
            <Button x:Name="btnCancelarNotificacion" Text="Cancelar Notificacion" HorizontalOptions="Center" VerticalOptions="CenterAndExpand" Clicked="BtnCancelarNotificacion_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>